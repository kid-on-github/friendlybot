{
    "name": "Email Finder",
    "description": "Find those emails!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "js": ["findEmails.js"],
            "matches": ["https://*/*", "http://*/*"],
            "run_at" : "document_end"
        }
    ]
}
